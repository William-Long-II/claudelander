{
  "name": "claudelander",
  "version": "1.1.0",
  "description": "Cross-platform Claude Code session manager - There Can Be Only One!",
  "homepage": "https://github.com/William-Long-II/claudelander",
  "author": {
    "name": "Will Long II",
    "email": "sytanek@gmail.com"
  },
  "contributors": [
    {
      "name": "Claude (Anthropic)",
      "email": "noreply@anthropic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/William-Long-II/claudelander.git"
  },
  "main": "dist/main/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --mode production",
    "build": "npm run build:main && npm run build:renderer",
    "start": "npm run build && electron .",
    "dev": "npm run build && electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "dist:linux": "npm run build && electron-builder --linux"
  },
  "keywords": [],
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "node-pty": "^1.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-webgl": "^0.16.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.6.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "build": {
    "extends": "electron-builder.yml"
  }
}
